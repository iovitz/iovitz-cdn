{
  "name": "iovitz-cdn",
  "version": "1.0.1",
  "description": "当做静态文件CDN使用",
  "main": "index.js",
  "scripts": {
    "html": "./node_modules/.bin/webpack serve",
    "dev": "npm run build && ./node_modules/.bin/concurrently \"npm run exec\" \"npm run build:watch\"",
    "exec": "./node_modules/.bin/nodemon ./dist/main.js",
    "build:watch": "./node_modules/.bin/webpack build --watch",
    "build": "./node_modules/.bin/webpack build"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "author": "iovitz",
  "license": "ISC",
  "devDependencies": {
    "@types/jquery": "^3.5.30",
    "@types/node": "^20.8.7",
    "concurrently": "^8.2.2",
    "html-webpack-plugin": "^5.5.3",
    "nodemon": "^3.0.1",
    "reflect-metadata": "^0.1.13",
    "ts-loader": "^9.5.0",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "@types/mousetrap": "^1.6.15",
    "jquery": "^3.7.1",
    "mousetrap": "^1.6.5"
  }
}
